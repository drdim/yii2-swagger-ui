    "keywords": [
        "php",
        "yii2",
        "swagger-ui",
        "swagger",
        "extension",
        "component"
    ],
    "homepage": "https://github.com/veksa/yii2-swagger-ui",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Khijnij",
            "email": "admin@devblog.pro",
            "homepage": "https://devblog.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "yiisoft/yii2": "^2.0",
        "bower-asset/components-underscore": "^1.8.3",
        "bower-asset/swagger-ui": "2.2.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
